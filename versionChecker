local data = {
    version = nil
}

local latestVersion = "0.9"
local latestBeta = "1.0 beta"
local latestPrivateBeta = "1.0 beta"

function data.checkVersion()
    if data.version ~= latestVersion and data.version ~= latestBeta and data.version ~= latestPrivateBeta then
        game:GetService("Players").LocalPlayer:Kick("you are using an outdated version of Project Bot, join our discord for the latest version - jailbreak.projectbot.top")
    end
end

return data
